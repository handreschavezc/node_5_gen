<body>
 


  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Categoria</th>
        <th scope="col">Producto</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <th scope="row">{{id}}</th>
        <td>{{categoria}}</td>
        <td>{{producto}}</td>
        
      </tr>
      {{/each}}
    </tbody>
  </table>
  
  
</body>
